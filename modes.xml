<?xml version="1.0" encoding="UTF-8"?>
<modes>

  <mode name="tr-az" install="yes">
    <pipeline>
      <program name="hfst-proc">
        <file name="tr-az.automorf.hfst"/>
      </program>
      <program name="cg-proc">
        <file name="tr-az.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="tr-az.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-tr-az.tr-az.t1x"/>
        <file name="tr-az.t1x.bin"/>
        <file name="tr-az.autobil.bin"/>
      </program>
      <program name="hfst-proc $1">
        <file name="tr-az.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>
 
  <mode name="tr-az-debug" install="no">
    <pipeline>
      <program name="hfst-proc">
        <file name="tr-az.automorf.hfst"/>
      </program>
      <program name="cg-proc">
        <file name="tr-az.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="tr-az.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-tr-az.tr-az.t1x"/>
        <file name="tr-az.t1x.bin"/>
        <file name="tr-az.autobil.bin"/>
      </program>
      <program name="hfst-proc -d">
        <file name="tr-az.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>
  
  <mode name="tr-az-morph" install="no">
    <pipeline>
      <program name="hfst-proc">
        <file name="tr-az.automorf.hfst"/>
      </program>
    </pipeline>
  </mode>

  <mode name="tr-az-cg" install="no">
    <pipeline>
      <program name="hfst-proc">
        <file name="tr-az.automorf.hfst"/>
      </program>
      <program name="cg-proc">
        <file name="tr-az.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="tr-az-cg-trace" install="no">
    <pipeline>
      <program name="hfst-proc">
        <file name="tr-az.automorf.hfst"/>
      </program>
      <program name="tagger-to-visl.py"/>
      <program name="vislcg3 -tg">
        <file name="tr-az.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="tr-az-tagger" install="no">
    <pipeline>
      <program name="hfst-proc">
        <file name="tr-az.automorf.hfst"/>
      </program>
      <program name="cg-proc">
        <file name="tr-az.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="tr-az.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
    </pipeline>
  </mode>

  <mode name="tr-az-bil" install="no">
    <pipeline>
      <program name="hfst-proc">
        <file name="tr-az.automorf.hfst"/>
      </program>
      <program name="cg-proc">
        <file name="tr-az.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="tr-az.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="lt-proc -b">
        <file name="tr-az.autobil.bin"/>
      </program>
    </pipeline>
  </mode>


  <mode name="tr-az-transfer" install="no">
    <pipeline>
      <program name="hfst-proc">
        <file name="tr-az.automorf.hfst"/>
      </program>
      <program name="cg-proc">
        <file name="tr-az.rlx.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="tr-az.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-tr-az.tr-az.t1x"/>
        <file name="tr-az.t1x.bin"/>
        <file name="tr-az.autobil.bin"/>
      </program>
    </pipeline>
  </mode>
</modes>



